{
  "name": "exquisite-corpse",
  "version": "1.0.0",
  "main": "index.js",
  "repository": {},
  "license": "MIT",
  "private": true,
  "engines": {
    "node": "6.10.2"
  },
  "scripts": {
    "start": "node index.js",
    "install:local": "npm install",
    "test": "NODE_ENV=test jest",
    "test:coverage": "yarn test -- --coverage",
    "test:watch": "npm run test -- --watch",
    "test:watch:coverage": "npm run test:coverage -- --watch",
    "test:snapshots:update": "npm run test -- --updateSnapshot",
    "flow": "flow status",
    "dev": "npm-run-all --parallel server webpack:dev",
    "storybook": "start-storybook -p 9001",
    "server": "nodemon index.js",
    "client": "NODE_PATH=./src/ npm start --prefix client",
    "webpack:dev": "rm -rf ./client/build | NODE_ENV=development webpack --env=dev --progress --watch",
    "webpack:prod": "rm -rf ./client/build | NODE_ENV=production webpack --progress --env=prod",
    "build:dev": "npm run webpack:dev -- --progress --profile --colors",
    "build:prod": "npm run webpack:prod -- --progress --profile --colors",
    "postinstall": "npm run build:prod",
    "analyze": "NODE_ENV=production webpack --env=prod --progress --json | webpack-bundle-size-analyzer",
    "console": "node server/console.js"
  },
  "dependencies": {
    "aws-sdk": "^2.39.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^6.4.1",
    "babel-plugin-ramda": "^1.2.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "bell": "^8.5.0",
    "boom": "^4.2.0",
    "concurrently": "^3.4.0",
    "cross-env": "^4.0.0",
    "css-loader": "^0.27.3",
    "date-fns": "^1.28.2",
    "dotenv": "^4.0.0",
    "eslint-plugin-flowtype": "^2.32.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "flow-bin": "^0.45.0",
    "flow-status-webpack-plugin": "^0.1.7",
    "flow-typed": "^2.1.2",
    "glamor": "^2.20.24",
    "glue": "^4.1.0",
    "good": "^7.1.0",
    "good-console": "^6.4.0",
    "good-squeeze": "^5.0.1",
    "h2o2": "^5.4.0",
    "hapi": "^16.1.0",
    "hapi-auth-cookie": "^6.1.1",
    "hapi-mongodb": "^6.2.1",
    "hapi-swagger": "^7.7.0",
    "html-webpack-plugin": "^2.28.0",
    "inert": "^4.1.0",
    "joi": "^10.2.2",
    "mongodb": "^2.2.24",
    "nes": "^6.4.2",
    "npm-run-all": "^4.0.2",
    "nunjucks": "^3.0.0",
    "paper": "^0.10.3",
    "prop-types": "^15.5.7",
    "ramda": "^0.23.0",
    "rc-slider": "^7.0.1",
    "react": "^15.4.2",
    "react-boxen": "1.1.2",
    "react-dom": "^15.4.2",
    "react-md-spinner": "^0.2.1",
    "react-redux": "^5.0.4",
    "react-router": "^4.0.0",
    "react-router-dom": "^4.0.0",
